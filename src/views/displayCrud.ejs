<!DOCTYPE html>
<html>
  <head>
    <style>
      table {
        font-family: arial, sans-serif;
        border-collapse: collapse;
        width: 100%;
      }

      td,
      th {
        border: 1px solid #dddddd;
        text-align: left;
        padding: 8px;
      }

      tr:nth-child(even) {
        background-color: #dddddd;
      }
    </style>
  </head>
  <body>
    <h2>HTML Table</h2>
    <table>
      <tr>
        <th>Email</th>
        <th>FirstName</th>
        <th>LastName</th>
        <th>Address</th>
        <th>PhoneNumber</th>
        <th>Gender</th>
      </tr>
      <% for(var i=0; i < data.length; i++) { %>
      <tr>
        <td><%= data[i].email%></td>
        <td><%= data[i].firstName %></td>
        <td><%= data[i].lastName %></td>
        <td><%= data[i].address%></td>
        <td><%= data[i].phoneNumber %></td>
        <td><%= data[i].gender %></td>
      </tr>
      <% } %>
    </table>
  </body>
</html>
